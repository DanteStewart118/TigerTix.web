@model IndexViewModel
@{
    ViewBag.Title = "Home Page";

}
<!DOCTYPE html>
<html>
<head>
    <title>TigerTix: @ViewBag.Title</title>
</head>
<body style="background-color:floralwhite">
    <h1>Tiger Tix</h1>

    <form method="post">
        <label>Username:</label>
        <br />
        <input type="text" name="UserName" />
        <br />
        <label>First name:</label>
        <br />
        <input type="text" name="FirstName" />
        <br />
        <label>Last name:</label>
        <br />
        <input type="text" name="LastName" />
        <br />
        <input type="submit" value="Store Name" />
    </form>
    <br />
    <br />
    <br />
    <br />
    <br />
    <button onclick="window.location.href = '/App/ShowUsers'">Show users</button>
    <button onclick="window.location.href = '/Events/Index'">Want to Submit an Event?</button>
    <button onclick="window.location.href = '/Events/Eventlist'">Event List</button>
    <button onclick="window.location.href = '/Cart/Index'">Go to cart</button>
   
</body>

</html>
